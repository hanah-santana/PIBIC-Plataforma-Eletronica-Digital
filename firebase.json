{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "hosting": {
      "redirects": [ {
        "source": "/index.html",
        "destination": "/path/index.html",
        "type": 301
      }]
    },
    "cleanUrls": true
  } 
}
